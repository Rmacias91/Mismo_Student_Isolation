<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.richardmacias.cs6460">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.richardmacias.cs6460.features.MainMeetList.activites.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.SEARCH" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <meta-data android:name="android.app.searchable"
                android:resource="@xml/searchable"/>
        </activity>
        <activity android:name="com.example.richardmacias.cs6460.features.AddMeet.AddMeetActivity" />
        <activity android:name="com.example.richardmacias.cs6460.features.DetailMeet.DetailMeetActivity" />
        <activity android:name=".features.ContentList.activites.ContentListActivity" />
        <activity android:name=".features.ArticleDetail.ArticleDetailActivity" />
        <activity android:name=".features.VideoDetail.VideoDetailActivity"
            android:screenOrientation="landscape"  />

    </application>

</manifest>